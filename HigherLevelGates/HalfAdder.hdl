/**
 * 2-bit bitwise HalfAdder:
 */

CHIP And16 {
    IN a, b;
    OUT c, s;

    PARTS:
    // Put your code here:
    X0r( a = a, b = b, out = s);
    And( a = a, b = b, out = c);
}